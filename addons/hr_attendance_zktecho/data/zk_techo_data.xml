<?xml version="1.0" ?>
<odoo>
  <data noupdate="1">

	<record id="biometric_attendances" model="ir.cron">
		<field name="name">Biometric Attendance</field>
		<field name="priority">5</field>
		<field name="doall">False</field>
		<field name="active">True</field>
		<field name="numbercall">-1</field>
		<field name="interval_number">1</field>
		<field name="interval_type">days</field>
		<field name="model_id" ref="model_biomteric_device_info"/>
		<field name="state">code</field>
		<field name="code">model.fetch_attendance()</field>
	</record>

	<record id="ir_cron_data_moves_update" model="ir.cron">
            <field name="name">Move Checkin/Chekout</field>
            <field name="model_id" ref="model_hr_draft_attendance"/>
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">model.update_move_confirm()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
        </record>
        
  </data>
</odoo>
